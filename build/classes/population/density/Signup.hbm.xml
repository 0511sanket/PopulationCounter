<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="population.density">
    <class name="Signup" table="Signup">
        <id name="regNo" column="regNo">
        <!--<generator class = "native"/>-->
        </id>
        <property name="name" column="name"/>
        <property name = "city" column="city"/>
        <property name="pincode" column="pincode"/>
        <property name="contactNo" column="contactNo"/>
        <property name="age" column="age"/>
        <property name="password" column="password"/>
        <property name="confirmPassword" column="confirmPassword"/>
    </class>
</hibernate-mapping>
