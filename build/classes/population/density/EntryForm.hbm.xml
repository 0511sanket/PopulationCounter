<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping  package="population.density">
    <class name="EntryForm" table="EntryForm">
        <id name="regNo" column="regNo"></id>
        <property  name="name" column="name"></property>
        <property  name="age" column="age"></property>
        <property  name="gender" column="gender"></property>
        <property  name="city" column="city"></property>
        <property  name="pincode" column="pincode"></property>
        <property  name="address" column="address"></property>
        <property  name="contactNo" column="contactNo"></property>
    </class>
</hibernate-mapping>
